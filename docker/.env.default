DEV_DSF_FHIR_STORE_LISTEN_ADDRESS_DIC_2=127.0.0.1:8081
DEV_DSF_FHIR_STORE_TYPE_DIC_2=blaze # valid values are 'blaze' and 'hapi'

# DMS
DEV_DSF_BPE_DEBUG_LISTEN_ADDRESS_DMS=127.0.0.1:5012
DEV_DSF_BPE_PROCESS_DIR_DMS=./dms/bpe/process
DEV_DSF_FHIR_DEBUG_LISTEN_ADDRESS_DMS=127.0.0.1:5002
DEV_DSF_FHIR_STORE_LISTEN_ADDRESS_DMS=127.0.0.1:8082
