# General
CPU_ARCHITECTURE=amd64 # one of [amd64, i386, aarch64, armv7, armhf, ppc64le]
DEV_DSF_BPE_IMAGE=ghcr.io/datasharingframework/bpe:1.5.0
DEV_DSF_FHIR_IMAGE=ghcr.io/datasharingframework/fhir:1.5.0

# Proxy
DEV_DSF_PROXY_LISTEN_ADDRESS=127.0.0.1:443

# DIC 1
DEV_DSF_BPE_DEBUG_LISTEN_ADDRESS_DIC_1=127.0.0.1:5010
DEV_DSF_BPE_PROCESS_DIR_DIC_1=./dic1/bpe/process
DEV_DSF_FHIR_DEBUG_LISTEN_ADDRESS_DIC_1=127.0.0.1:5000
DEV_DSF_FHIR_STORE_LISTEN_ADDRESS_DIC_1=127.0.0.1:8080
DEV_DSF_FHIR_STORE_TYPE_DIC_1=blaze # valid values are 'blaze' and 'hapi'

# DIC 2
DEV_DSF_BPE_DEBUG_LISTEN_ADDRESS_DIC_2=127.0.0.1:5011
DEV_DSF_BPE_PROCESS_DIR_DIC_2=./dic2/bpe/process
